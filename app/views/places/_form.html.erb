<%= simple_form_for(@place) do |f| %>
<%= f.label :district_id %><br />
<%= f.collection_select :district_id, District.all, :id, :name, :prompt => true %>
<br/>
<%= f.label :name %><br />
<%= f.text_field :name %>
    <br/>
<%= f.label :address %><br />
<%= f.text_field :address %>
    <br/>
<%= f.label :longitud %><br />
<%= f.number_field :longitud %>
    <br/>
<%= f.label :latitude %><br />
<%= f.number_field :latitude %>
    <br/>
<%= f.label :phone %><br />
<%= f.text_field :phone %>
    <br/>
<%= f.label :imagen %><br />
<%= f.text_field :imagen %>
    <br/>
<%= f.submit %>
<% end %>
